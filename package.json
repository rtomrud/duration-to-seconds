    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "rollup": "2.56.2"
  },
  "type": "module",
  "exports": {
    "import": "./index.js",
    "default": "./dist/index.cjs"
  }
