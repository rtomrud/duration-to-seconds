{
  "name": "duration-to-seconds",
  "version": "1.0.0-rc.1",
  "description": "Converts an ISO 8601 duration to seconds",
  "keywords": [
    "convert",
    "date",
    "duration",
    "ISO",
    "ISO 8601",
    "seconds",
    "time",
    "unit"
  ],
  "homepage": "https://github.com/rtomrud/duration-to-seconds#readme",
  "bugs": {
    "url": "https://github.com/rtomrud/duration-to-seconds/issues"
  },
  "license": "MIT",
  "author": "Artem Rudenko <rtomrud@gmail.com>",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rtomrud/duration-to-seconds.git"
  },
  "scripts": {
    "lint": "eslint *.js",
    "test": "nyc --reporter html tape -r esm index.test.js | faucet && nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-pretty-strict": "^1.0.0-rc.15",
    "eslint-plugin-prettier": "^3.0.0",
    "esm": "^3.0.84",
    "faucet": "0.0.1",
    "husky": "^1.1.4",
    "nyc": "^13.1.0",
    "prettier": "1.15.2",
    "tape": "^4.9.1"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/eslint-config-pretty-strict/index.js"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "root": true
  },
  "esm": {
    "cjs": true,
    "mode": "auto"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
