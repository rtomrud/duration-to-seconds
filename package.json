{
  "name": "duration-to-seconds",
  "version": "1.0.0",
  "description": "Converts an ISO 8601 duration to seconds",
  "keywords": [
    "convert",
    "date",
    "duration",
    "ISO",
    "ISO 8601",
    "seconds",
    "time",
    "unit"
  ],
  "homepage": "https://github.com/rtomrud/duration-to-seconds#readme",
  "bugs": {
    "url": "https://github.com/rtomrud/duration-to-seconds/issues"
  },
  "license": "MIT",
  "author": "Artem Rudenko <rtomrud@gmail.com>",
  "files": [
    "dist",
    "index.js"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rtomrud/duration-to-seconds.git"
  },
  "scripts": {
    "build": "rollup -i index.js -o dist/index.js -f cjs",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "prepare": "npm run build",
    "test": "nyc --reporter html tape -r esm index.test.js | faucet && nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "eslint-config-pretty-strict": "2.0.0",
    "eslint-plugin-prettier": "3.0.1",
    "esm": "3.2.22",
    "faucet": "0.0.1",
    "husky": "2.1.0",
    "nyc": "14.0.0",
    "prettier": "1.17.0",
    "rollup": "1.10.1",
    "tape": "^4.10.1"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/eslint-config-pretty-strict/index.js"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "root": true
  },
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "esm": {
    "sourceMap": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
